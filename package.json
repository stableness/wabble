{
  "name": "@stableness/wabble",
  "version": "0.6.2",
  "description": "http/socks5 proxy client with rules filtering and DoH support in Node.js",
  "esm": "dist/index.mjs",
  "main": "dist/index.cjs",
  "scripts": {
    "build": "tsc && rollup -c",
    "start": "DEV_PORT_MINUS=1000 node dist/bin.js | pino-pretty -t SYS:HH:MM:ss",
    "non.tsc.start": "NODE_ENV=dev rollup -c && npm start",
    "dev": "TSC_NONPOLLING_WATCHER=true tsc-watch --onSuccess 'npm run -s non.tsc.start'",
    "bundle": "NODE_ENV=production npm run build -- --environment BUILD:prod",
    "test": "jest --ci --coverage -c jest.config.json"
  },
  "bin": {
    "wabble": "dist/bin.cjs"
  },
  "files": [
    "dist/bin.cjs",
    "dist/index.cjs"
  ],
  "repository": "github:stableness/wabble",
  "homepage": "https://github.com/stableness/wabble",
  "bugs": {
    "url": "https://github.com/stableness/wabble/issues"
  },
  "license": "GPL-3.0",
  "sideEffects": false,
  "dependencies": {
    "async-readable": "~0.2.2",
    "basic-auth": "github:stableness/basic-auth",
    "buffer-pond": "~0.8.0",
    "command-line-args": "^5.1.1",
    "dohdec": "github:stableness/dohdec",
    "fp-ts": "^2.5.4",
    "futoin-hkdf": "^1.2.1",
    "ip": "^1.1.5",
    "js-yaml": "^3.13.1",
    "pino": "^6.2.1",
    "proxy-bind": "^1.2.1",
    "ramda": "~0.27",
    "rxjs": "7.0.0-beta.0",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/command-line-args": "^5.0.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.9",
    "@types/pino": "^6.0.0",
    "@types/ramda": "~0.27",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "pino-pretty": "^4.0.0",
    "rollup": "^2.8.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-visualizer": "^4.0.4",
    "ts-jest": "^26.0.0",
    "tsc-watch": "^4.0.0",
    "typescript": "~3.8.3"
  }
}
