{
  "name": "@stableness/wabble",
  "version": "0.8.5",
  "description": "http/socks5 proxy client with rules filtering and DoH support in Node.js",
  "esm": "dist/index.mjs",
  "main": "dist/index.cjs",
  "scripts": {
    "build": "tsc && rollup -c",
    "start": "DEV_PORT_MINUS=1000 node dist/bin.js | pino-pretty -t SYS:HH:MM:ss",
    "non.tsc.start": "NODE_ENV=dev rollup -c && npm start",
    "dev": "TSC_NONPOLLING_WATCHER=true tsc-watch --onSuccess 'npm run -s non.tsc.start'",
    "bundle": "NODE_ENV=production npm run build -- --environment BUILD:prod",
    "test": "jest --ci --coverage -c jest.config.json"
  },
  "bin": {
    "wabble": "dist/bin.cjs"
  },
  "files": [
    "tsconfig.json",
    "dist/extra.cjs",
    "dist/bin.cjs",
    "dist/index.cjs"
  ],
  "repository": "github:stableness/wabble",
  "homepage": "https://github.com/stableness/wabble",
  "bugs": {
    "url": "https://github.com/stableness/wabble/issues"
  },
  "license": "GPL-3.0",
  "sideEffects": false,
  "dependencies": {
    "@stableness/basic-auth": "^102.0.1",
    "@stableness/dohdec": "^102.1.0",
    "async-readable": "~0.4.2",
    "buffer-pond": "~0.8.2",
    "command-line-args": "^5.1.1",
    "fp-ts": "^2.8.2",
    "futoin-hkdf": "^1.2.1",
    "ip": "^1.1.5",
    "js-yaml": "^3.13.1",
    "pino": "^6.4.1",
    "proxy-bind": "^1.2.1",
    "ramda": "~0.27.1",
    "rxjs": "7.0.0-beta.5",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/command-line-args": "^5.0.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.3",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.9",
    "@types/pino": "^6.0.0",
    "@types/ramda": "~0.27.17",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "jest-junit": "^11.1.0",
    "pino-pretty": "^4.1.0",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.1.1",
    "ts-jest": "^26.3.0",
    "tsc-watch": "^4.0.0",
    "typescript": "~4.0.2"
  }
}
