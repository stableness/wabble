{
  "name": "@stableness/wabble",
  "version": "0.13.0",
  "description": "http/socks5 proxy client with rules filtering and DoH support in Node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "build-commonjs": "tsc --module CommonJS",
    "start": "DEV_PORT_MINUS=1000 NODE_ENV=dev node dist/bin.js | pino-pretty -t SYS:HH:MM:ss",
    "dev": "TSC_NONPOLLING_WATCHER=true tsc-watch --build --onSuccess 'npm run -s start'",
    "bundle": "npm run build && NODE_ENV=production rollup -c",
    "lint": "eslint ./ --cache --cache-location ./dist/ --ext .ts -c .eslintrc.yml",
    "test": "jest --ci -c jest.config.json"
  },
  "bin": {
    "wabble": "dist/bin.cjs"
  },
  "files": [
    "tsconfig.json",
    "dist/**/*.js",
    "dist/bin.cjs"
  ],
  "engines": {
    "node": ">= 12"
  },
  "type": "module",
  "repository": "github:stableness/wabble",
  "homepage": "https://github.com/stableness/wabble",
  "bugs": {
    "url": "https://github.com/stableness/wabble/issues"
  },
  "license": "GPL-3.0",
  "sideEffects": false,
  "dependencies": {
    "@stableness/basic-auth": "^102.0.1",
    "@stableness/dohdec": "^103.2.0",
    "async-readable": "~0.4.2",
    "buffer-pond": "~0.8.2",
    "command-line-args": "^5.1.1",
    "fp-ts": "^2.12.0",
    "fp-ts-std": "~0.13.1",
    "futoin-hkdf": "^1.2.1",
    "io-ts": "^2.2.15",
    "ip": "^2.0.0",
    "js-yaml": "^4.0.0",
    "memoizerific": "^1.11.3",
    "monocle-ts": "^2.3.10",
    "newtype-ts": "^0.3.4",
    "node-fetch": "^2.6.1",
    "pino": "^6.4.1",
    "proxy-bind": "^1.2.1",
    "ramda": "~0.27.1",
    "rfc4648": "^1.5.0",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/ip": "^1.1.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^12.20.13",
    "@types/node-fetch": "^2.5.7",
    "@types/pino": "^6.3.8",
    "@types/ramda": "~0.27.39",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^28.1.0",
    "jest-junit": "^13.2.0",
    "nock": "^13.0.5",
    "p-defer": "^3.0.0",
    "pino-pretty": "^5.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.0.3",
    "ts-jest": "^28.0.2",
    "tsc-watch": "^4.0.0",
    "typescript": "~4.6.4"
  }
}
